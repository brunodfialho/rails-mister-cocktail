<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://media-cdn.tripadvisor.com/media/photo-s/19/71/51/02/photo0jpg.jpg);">
  <div class="container">
    <h1>Welcome to <strong>Mister cocktails</strong></h1>
    <%= link_to 'Create a cocktail', new_cocktail_path, class:"btn btn-primary" %>
  </div>
</div>
<%@cocktails.each do |cocktail|%>
<div class="card-product">
  <img src="data:image/jpeg;base64," />
  <div class="card-product-infos">
    <h2><%=cocktail.name%></h2>
    <%= link_to 'Get more information', cocktail_path(cocktail), class:"btn btn-warning" %>
  </div>
</div>

<%end%>